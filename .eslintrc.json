{
    "extends": ["airbnb", "prettier"],
    "plugins": ["prettier"],
    "rules": {
        "prettier/prettier": ["error"],
        "no-unused-vars": "warn",
        "no-console": "off",
        "prefer-const": "warn",
        "func-names": "off",
        "radix": "off",
        "consistent-return": "off"
        // "no-var": 0
    }
}
